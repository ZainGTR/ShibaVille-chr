@test module;
import ^^.main.{create_token, get_all_tokens};


function test_add_token() {
    //assert_equals(my_name.name, "World");

    val tx = rell.test.tx(create_token("bonds", "BND"));
    //assert_equals(my_name.name, "World");

    tx.run();
    val tokens = get_all_tokens();
    assert_equals(tokens.size(), 1);
}
